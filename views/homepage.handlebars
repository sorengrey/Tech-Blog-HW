<!-- Loop over each object in the blog array -->
{{#each Blog as |blog| }}
<div class="row mb-4 blog">
  <div class="col-md-5">
    <h2>
      <!-- Call helper function with triple brackets to render the span as HTML -->
      <a href="/blog/{{blog.id}}">{{blog.title}}</a>
    </h2>
  </div>
</div>
{{/each}}
